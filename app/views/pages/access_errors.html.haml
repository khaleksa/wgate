.container.mtop50
  .row.placeholders

  %h2.sub-header Access errors for itest.uz
  %hr

  .table-responsive
    %table.table.table-striped
      %thead
        %tr
          %th #
          %th Timestamp
          %th Payment System
          %th Wrong Account
      %tbody
        -@account_errors.each_with_index do |error, index|
          %tr
            %td= index + 1
            %td= error.created_at
            %td= error.payment_system
            %td= error.account_id
